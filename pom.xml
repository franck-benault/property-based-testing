<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>net.franckbenault.propertybasedtesting</groupId>
	<artifactId>property-based-testing</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<packaging>pom</packaging>
	<properties>
		<junit-quickcheck.version>0.6</junit-quickcheck.version>
	</properties>
	<dependencyManagement>
		<dependencies>
			<dependency>
				<groupId>junit</groupId>
				<artifactId>junit</artifactId>
				<scope>test</scope>
				<version>4.12</version>
			</dependency>
			<dependency>
				<groupId>org.testng</groupId>
				<artifactId>testng</artifactId>
				<scope>test</scope>
				<version>6.9.10</version>
			</dependency>
			<dependency>
				<groupId>com.pholser</groupId>
				<artifactId>junit-quickcheck-core</artifactId>
				<scope>test</scope>
				<version>${junit-quickcheck.version}</version>
			</dependency>
			<dependency>
				<groupId>com.pholser</groupId>
				<artifactId>junit-quickcheck-generators</artifactId>
				<scope>test</scope>
				<version>${junit-quickcheck.version}</version>
			</dependency>
			<dependency>
				<groupId>com.theoryinpractise</groupId>
				<artifactId>quickcheckng</artifactId>
				<scope>test</scope>
				<version>1.1.4</version>
			</dependency>
			<dependency>
				<groupId>org.quicktheories</groupId>
				<artifactId>quicktheories</artifactId>
				<scope>test</scope>
				<version>0.13</version>
			</dependency>
			<dependency>
				<groupId>org.hamcrest</groupId>
				<artifactId>hamcrest-all</artifactId>
				<version>1.3</version>
			</dependency>
			<dependency>
				<groupId>org.slf4j</groupId>
				<artifactId>slf4j-api</artifactId>
				<version>1.7.16</version>
				<scope>compile</scope>
			</dependency>
			<dependency>
				<groupId>org.slf4j</groupId>
				<artifactId>slf4j-simple</artifactId>
				<version>1.7.16</version>
				<scope>runtime</scope>
			</dependency>
			<dependency>
				<groupId>org.quicktheories</groupId>
				<artifactId>quicktheories</artifactId>
				<version>0.13</version>
			</dependency>
		</dependencies>
	</dependencyManagement>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.1</version>
				<configuration>
					<source>1.8</source>
					<target>1.8</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<modules>
		<module>001-Annotation-theory</module>
		<module>002-JUnit-Quickcheck</module>
		<module>003-QuickTheory</module>
		<module>001b-Dataprovider-testNG</module>
		<module>002b-Quickcheckng</module>
	</modules>
</project>